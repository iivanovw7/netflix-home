{
    "name": "netflix-home",
    "version": "1.0.0",
    "description": "Netflix browse webpage",
    "directories": {
        "assets": "assets",
        "config": "config",
        "src": "src",
        "tool": "tool"
    },
    "main": "src/main.tsx",
    "scripts": {
        "build": "run-s \"build:server -- {@}\" --",
        "build:server": "node ./tool/build.js",
        "dev": "run-s \"dev:server -- {@}\" -- --source-maps",
        "dev:server": "node ./tool/server.js",
        "doc": "typedoc --options ./config/typedoc/typedoc.js",
        "lint": "run-s \"lint-css\" \"lint-eslint\"",
        "lint-css": "stylelint \"./src/**/*.{css,pcss,scss}\" --formatter verbose --cache",
        "lint-css:fix": "stylelint \"./src/css/**/*.{css,pcss,scss}\" --formatter verbose --fix --cache",
        "lint-eslint": "eslint . --cache --max-warnings 0 --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-pattern internals/scripts --ignore-path .eslintignore",
        "lint-eslint:fix": "eslint . --cache --max-warnings 0 --ext .js --ext .jsx --ext .ts --ext .tsx --ignore-pattern internals/scripts --ignore-path .eslintignore --fix",
        "reset": "rm -rf build && rm -rf node_modules && rm -rf package-lock.json && rm -rf pnpm-lock",
        "test": "jest --config jest.config.js",
        "coverage": "npm test -- --coverage",
        "test:watch": "npm run test -- --watch",
        "view": "node ./tool/view.mjs",
        "pre-commit": "run-s \"lint\" \"test\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "classnames": "2.3.2",
        "history": "5.3.0",
        "loglevel": "1.8.0",
        "mobx": "6.6.2",
        "mobx-react-lite": "3.4.0",
        "mobx-state-tree": "5.1.6",
        "ramda": "^0.28.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-router": "6.4.0",
        "react-router-dom": "6.4.0",
        "store2": "2.14.2",
        "uuid": "9.0.0",
        "web-vitals": "3.0.1",
        "yup": "0.32.11"
    },
    "devDependencies": {
        "@babel/core": "7.19.3",
        "@babel/eslint-parser": "7.19.1",
        "@babel/eslint-plugin": "7.19.1",
        "@babel/preset-env": "7.19.4",
        "@babel/preset-react": "7.18.6",
        "@getify/eslint-plugin-proper-arrows": "11.0.3",
        "@testing-library/dom": "8.19.0",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/jest": "29.1.2",
        "@types/node": "18.8.3",
        "@types/ramda": "0.28.15",
        "@types/react": "18.0.18",
        "@types/react-dom": "18.0.6",
        "@types/react-router": "5.1.19",
        "@types/react-router-dom": "5.3.3",
        "@typescript-eslint/eslint-plugin": "5.40.0",
        "@typescript-eslint/parser": "5.40.0",
        "@vitejs/plugin-basic-ssl": "0.1.2",
        "@vitejs/plugin-legacy": "2.1.0",
        "@vitejs/plugin-react": "2.1.0",
        "autoprefixer": "10.4.11",
